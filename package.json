{
  "name": "browser-extvet",
  "version": "0.7.0",
  "description": "Browser Extension Security Scanner - Vet your extensions before they vet your data",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "extvet": "bin/extvet.js"
  },
  "files": [
    "bin/",
    "dist/",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "npm run build && node --test test/*.test.js",
    "start": "node bin/extvet.js",
    "prepublishOnly": "npm run build && npm test",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "browser-extension",
    "chrome-extension",
    "firefox-addon",
    "security",
    "scanner",
    "manifest",
    "permissions",
    "audit",
    "typescript"
  ],
  "author": "tez",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taku-tez/ExtVet.git"
  },
  "bugs": {
    "url": "https://github.com/taku-tez/ExtVet/issues"
  },
  "homepage": "https://github.com/taku-tez/ExtVet#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0"
  }
}
